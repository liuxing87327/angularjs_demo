<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>angularjs-demo</display-name>
  
  <servlet>
    <servlet-name>angularjs-demo</servlet-name>
    <servlet-class>com.dooioo.servlet.IndexServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>angularjs-demo</servlet-name>
    <url-pattern>/angularjs-demo/*</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>data-api</servlet-name>
    <servlet-class>com.dooioo.servlet.ApiServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>data-api</servlet-name>
    <url-pattern>/data/api/*</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>redirect.jsp</welcome-file>
  </welcome-file-list>
  
</web-app>